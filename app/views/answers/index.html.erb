<p id="notice"><%= notice %></p>

<h1>Listing Answers</h1>

<table>
  <thead>
    <tr>
      <th>Questionid</th>
      <th>Level</th>
      <th>Title</th>
      <th>Content</th>
      <th>Answeruser</th>
      <th>Filepath</th>
      <th>Liker</th>
      <th>Obligate1</th>
      <th>Obligate2</th>
      <th>Obligate3</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @answers.each do |answer| %>
      <tr>
        <td><%= answer.questionid %></td>
        <td><%= answer.level %></td>
        <td><%= answer.title %></td>
        <td><%= answer.content %></td>
        <td><%= answer.answeruser %></td>
        <td><%= answer.filepath %></td>
        <td><%= answer.liker %></td>
        <td><%= answer.obligate1 %></td>
        <td><%= answer.obligate2 %></td>
        <td><%= answer.obligate3 %></td>
        <td><%= link_to 'Show', answer %></td>
        <td><%= link_to 'Edit', edit_answer_path(answer) %></td>
        <td><%= link_to 'Destroy', answer, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Answer', new_answer_path %>
